{
  "name": "next-level",
  "version": "1.0.0",
  "description": "Do zero ao infinito, do nada à algum lugar, o próximo nível...",
  "main": "index.js",
  "scripts": {
    "dev": "npm run services:up && npm run services:wait:database && npm run migrations:up && next dev",
    "test": "npm run services:up && npm run services:wait:database && concurrently -n next,jest --hide next -k -s command-jest \"next dev\" \"jest --runInBand --verbose\"",
    "test:watch": "npm run services:up && npm run services:wait:database && concurrently -n next,jest --hide next -k -s command-jest \"next dev\" \"jest --watchAll --runInBand --verbose\"",
    "services:wait:database": "node infra/scripts/wait-for-postgres.js",
    "services:up": "docker compose -f infra/compose.yaml up -d",
    "services:down": "docker compose -f infra/compose.yaml down -v",
    "services:stop": "docker compose -f infra/compose.yaml stop",
    "migrations:create": "node-pg-migrate -m infra/migrations create",
    "migrations:up": "node-pg-migrate -m infra/migrations --envPath .env.development up",
    "lint:prettier:check": "prettier --check .",
    "lint:prettier:fix": "prettier --write .",
    "lint:eslint:check": "next lint --dir .",
    "lint:eslint:fix": "next lint --fix",
    "prepare": "husky",
    "commit": "cz"
  },
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@react-pdf/renderer": "^4.3.1",
    "@supabase/supabase-js": "^2.56.0",
    "async-retry": "^1.3.3",
    "bcrypt": "^6.0.0",
    "bcryptjs": "^3.0.2",
    "dotenv": "^16.6.1",
    "dotenv-expand": "^11.0.7",
    "formidable": "^3.5.4",
    "next": "^14.2.30",
    "next-auth": "^4.24.11",
    "node-pg-migrate": "^7.6.1",
    "or": "^0.2.0",
    "pg": "^8.16.3",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "swr": "2.2.5"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.8.1",
    "@commitlint/config-conventional": "^19.8.1",
    "commitizen": "^4.3.1",
    "concurrently": "^8.2.2",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^8.57.1",
    "eslint-config-next": "^14.2.3",
    "eslint-config-prettier": "^9.1.2",
    "eslint-plugin-jest": "^29.0.1",
    "husky": "^9.1.7",
    "jest": "^29.7.0",
    "prettier": "^3.6.2"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "overrides": {
    "braces": "3.0.3",
    "micromatch": "4.0.8",
    "minimist": "1.2.8",
    "lodash": "4.17.21",
    "shelljs": "0.8.5",
    "glob": "10.4.5",
    "inflight": "1.0.6",
    "rimraf": "3.0.2",
    "tmp": "0.2.3"
  }
}
